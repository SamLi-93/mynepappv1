<ion-view hide-tabs="true">
	<ion-nav-title>
		<!--label class="job_static">
	    	<input type="text" placeholder="搜索课程" ng-model="search.value">
	  	</label-->
  
      <div class="bar bar-header item-input-inset job_search">
        <label class="item-input-wrapper white">
          <i class="icon ion-ios-search placeholder-icon"></i>
          <input type="search" placeholder="Search" ng-model="search.value">
        </label>
      </div>
    </ion-nav-title>
	<ion-nav-buttons side="right">
  <button class="button button-clear job_button" ng-click="toresult()" > 搜索 </button>
  </ion-nav-buttons>
  	<ion-content>
  		<div class="list">
  			<a class="job_search_block" ng-repeat="search in searchlist" href="#/tab/result/{{search}}">
  				{{search}}
  			</a>
  		</div>
  	</ion-content>
</ion-view>
